params {
    config_profile_name        = 'elembio nfcore custom conf profile'
    config_profile_description = 'elembio nfcore custom conf profile'

    profiles {
        elembio_catalyst {
            includeConfig "${params.custom_config_base}/conf/elembio_catalyst.config"
        }
    }
}

aws {
   batch {
      cliPath = '/home/ec2-user/miniconda/bin/aws'
      maxParallelTransfers = 10
      maxTransferAttempts = 3
   }
   client {
      maxConnections = 10
      uploadMaxThreads = 10
      maxErrorRetry = 3
      socketTimeout = 3600000
      uploadRetrySleep = 1000
      uploadChunkSize = 32.MB
   }
}
